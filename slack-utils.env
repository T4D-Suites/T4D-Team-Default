_t4dSrcSlackSendToChannel(){
# Generated From $Tools4Dev_PATH/Engine/G4d/templates.env
# v1.7.0
###### _t4dSrcSlackSendToChannel
#   - Name:
#       _t4dSrcSlackSendToChannel
###
#   - Synopsis:
#       parse channel name with channels token generated by slack interface
###
#   - Note:
#       <Required>
#       [Optionnal]
#       {XXXXXXX}       = Value To Be Changed, if {URL} then replace {URL} with a valid URL
###
###### DOCUMENTATION BLOC #####
########### CODE BLOC #########
    local _t4dErrorCode=1
    local _ChannelName="${2:-sandbox}"
    local _Message="$1"
    local _Url

    case $_ChannelName in 
        sandbox)                _Url="$WS_SLACK_DEFAULT_CHANNEL"                   ;;
        *)
            #This is Jenkins Related Content
            if [[ "$M_SlackChannel" != "Empty" ]] && [[ "$M_SlackChannel" != "null" ]]; then
                _Url="$WS_SLACK_BASE_URL/$M_SlackChannel"
            fi
        ;;
    esac

    _t4dSrcSlackNotif "$_Message" "$_Url"

###############################
}